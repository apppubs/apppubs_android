<?xml version="1.0" encoding="utf-8"?>
<shape android:shape="rectangle"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 实心 -->
    <solid android:color="#00000000" />
    <!-- 描边 -->
    <stroke
        android:width="1dp"
        android:color="#8B8970" />
    <!-- 圆角 -->
    <corners android:radius="2dp" />
    <padding
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp" />
</shape>